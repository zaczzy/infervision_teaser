<script type="text/javascript" src="/paper/paper-core.js"></script>
<script type="text/javascript">
    window.onload = function () {
        var canvas = document.getElementById('myCanvas');
        paper.setup(canvas);
        var path = new paper.Path();
        path.strokeColor = 'red';
        var start = new paper.Point(100, 100);
        path.moveTo(start);
        path.lineTo(start.add([200, -50]));
        paper.view.draw();
    }
</script>
<canvas id="myCanvas" resize
        style="z-index: 10; position: fixed; background: url('/public/resource/1812010641.jpg'); width: 760px; height: 760px;"></canvas>
